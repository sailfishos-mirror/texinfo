# list secondary weights
 perl -wnle '/\[[\.\*]\d\d\d\d\.(\d\d\d\d)/ && print $1 ; ' <allkeys.txt  | sort | uniq

# list tertiary weights
 perl -wnle '/\[[\.\*]\d\d\d\d\.\d\d\d\d.(\d\d\d\d)/ && print $1 ; ' <allkeys.txt  | sort | uniq

