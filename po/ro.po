#~ "  makeinfo --html --no-headers foo.texi  write html without node lines, "
#~ "menus\n"
#~ "  makeinfo --number-sections foo.texi    write Info with numbered "
#~ "sections\n"
#~ "  makeinfo --no-split foo.texi           write one Info file however big\n"
